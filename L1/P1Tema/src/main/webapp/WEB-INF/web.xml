<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>ProcessStudent</servlet-name>
    <servlet-class>ProcessStudentServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AfisareStudenti</servlet-name>
    <servlet-class>AfisareStudentiServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SearchStudents</servlet-name>
    <servlet-class>SearchStudentsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteStudents</servlet-name>
    <servlet-class>DeleteStudentsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Export</servlet-name>
    <servlet-class>ExportServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>ProcessStudent</servlet-name>
    <url-pattern>/process-student</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AfisareStudenti</servlet-name>
    <url-pattern>/afisare-studenti</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchStudents</servlet-name>
    <url-pattern>/search-students</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteStudents</servlet-name>
    <url-pattern>/delete-students</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Export</servlet-name>
    <url-pattern>/export</url-pattern>
  </servlet-mapping>
</web-app>
